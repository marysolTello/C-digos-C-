//Lectura o entrada de datos

#include<iostream>

using namespace std;

int main(){
	int numero;
	
	cout<<"Digite un numero: ";
	//El numero que se ingresa se guarda en la variable
	cin>>numero;
	
	cout<<"\nEl numero que digito es: "<<numero;
	
	return 0;
}
